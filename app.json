{
    "expo": {
      "name": "SipSafe",
      "slug": "SipSafe",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/images/logo-sipsafe.png",
      "scheme": "sipsafe",
      "userInterfaceStyle": "automatic",
      "newArchEnabled": true,
      "ios": {
        "supportsTablet": true
      },
      "android": {
        "adaptiveIcon": {
          "backgroundColor": "#000000",
          "foregroundImage": "./assets/images/logo-sipsafe.png",
          "backgroundImage": "./assets/images/background.png",
          "monochromeImage": "./assets/images/logo-sipsafe.png"
        },
        "edgeToEdgeEnabled": true,
        "predictiveBackGestureEnabled": false
      },
      "web": {
        "output": "static",
        "favicon": "./assets/images/logo-sipsafe.png"
      },
      "plugins": [
        "expo-router",
        [
          "expo-image-picker",
          {
            "cameraPermission": "SipSafe uses your camera to verify drinks for signs of tampering."
          }
        ],
        [
          "expo-splash-screen",
          {
            "image": "./assets/images/background.png",
            "imageWidth": 1284,
            "resizeMode": "cover",
            "backgroundColor": "#000000",
            "dark": {
              "image": "./assets/images/background.png",
              "imageWidth": 1284,
              "resizeMode": "cover",
              "backgroundColor": "#000000"
            }
          }
        ],
        "expo-web-browser",
        "expo-font"
      ],
      "experiments": {
        "typedRoutes": true,
        "reactCompiler": true
      }
    }
  }